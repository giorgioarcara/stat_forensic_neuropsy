# Parameters
x_obs <- -1.64       # observed z-score
r_xx <- 0.64         # reliability
t_thresh <- -1.64    # true-score threshold (z-score)
sigma_T <- 1         # standard deviation of true scores (z-scale)
mu_T <- 0            # mean of true scores (z-scale)
# Conditional mean and standard deviation of true score
t_mean <- mu_T + r_xx * (x_obs - mu_T)          # E[T | X=x]
t_sd <- sigma_T * sqrt(1 - r_xx)               # SD[T | X=x]
# Probability that true score is below threshold
prob_below <- pnorm(t_thresh, mean = t_mean, sd = t_sd)
cat("Probability that true score < threshold:", round(prob_below, 4), "\n")
